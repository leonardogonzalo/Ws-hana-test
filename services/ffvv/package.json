{
  "name": "ffvv",
  "version": "1.0.0",
  "description": "Micro servicio que habilitara la contingencia ORACLE para Fuerza de Venta.",
  "main": "src/app.js",
  "scripts": {
    "clean": "rm -rf dist reports .nyc_output",
    "pretest": "npm run clean && npm run lint:report",
    "test": "cross-env NODE_ENV=test nyc mocha test/**/*.spec.js",
    "lint:console": "eslint src -f stylish",
    "lint:report": "eslint src -f html -o reports/lint.html",
    "prestart": "npm run clean && npm run lint:console",
    "start": "nodemon src/app.js",
    "build": "babel src --out-dir dist --copy-files && cp env.yaml dist/",
    "prestart:stage": "npm run clean && npm run build",
    "start:stage": "cross-env NODE_ENV=stage node ./dist/app.js",
    "prestart:production": "npm run clean && npm run build",
    "start:production": "cross-env NODE_ENV=production node ./dist/app.js"
  },
  "author": "Belcorp",
  "license": "MIT",
  "dependencies": {
    "@belcorp/logger": "^0.0.3",
    "axios": "^0.18.0",
    "cls-hooked": "^4.2.2",
    "execution-time": "^1.3.0",
    "koa": "^2.5.3",
    "koa-basic-auth": "^4.0.0",
    "koa-compress": "^3.0.0",
    "koa-helmet": "^4.0.0",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "node-hdb-pool": "^2.2.17",
    "node-uuid": "^1.4.8",
    "nodemon": "^1.18.7",
    "oracledb": "^3.0.0",
    "request": "^2.88.0",
    "tracer": "^0.9.8",
    "yenv": "^2.1.0",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.2.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-transform-arrow-functions": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-istanbul": "^5.0.1",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.6.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-prettier": "^3.0.0",
    "mocha": "^5.2.0",
    "mochawesome": "^3.0.3",
    "newrelic": "^4.9.0",
    "nyc": "^13.0.1",
    "prettier": "^1.14.3"
  }
}
